  robot.hear /dawson me (.+)/i, (msg) ->
    type = msg.match[1]
    img = dawson[type]
    msg.reply img

  robot.hear /dawson help/i, (msg) ->
    list = "/quote example : dawson me wink \nList of all commands:\n"
    for key,value of dawson
      list += " " + key
    msg.send list
